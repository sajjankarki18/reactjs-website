  <div className={`form-check form-switch text-${props.mode === 'dark'?'white':'dark'}`}>
              <input onChange={props.handleDarkmode} className="form-check-input" type="checkbox" id="flexSwitchCheckDefault"/>
              <label className="form-check-label" for="flexSwitchCheckDefault">{props.handleText}</label>
            </div>